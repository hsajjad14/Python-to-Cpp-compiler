def do_something(x: int, y: bool) -> int:
	l = [1,2,3,4]
	if x == 1000:
		if x == 100:
			if x < 102:
				if y:
					print(x)
		while y:
			if x == 0:
				x -=1
				y = False
			print(x)
	elif x < 0:
		return x + 100
	

	return x * 99

def print_sum(x: int, y: int, t: int):
	print(x+y+t)


x = do_something(x, False)
